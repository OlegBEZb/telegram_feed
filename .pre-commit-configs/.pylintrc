[FORMAT]

# Good variable names which should always be accepted, separated by a comma
good-names=i,j,k,x,y,df,c,X,f,ls,v,d,ch

[MESSAGES CONTROL]

disable=
    too-few-public-methods,
    missing-docstring,
    star-args,
    consider-using-f-string,
    import-error,
    missing-function-docstring,
    logging-fstring-interpolation,
    invalid-names,
    wrong-import-order,
    wrong-import-position,
    import-outside-toplevel,
    invalid-name,
    unspecified-encoding,
    use-dict-literal,
    unused-import,
    bare-except,  # remove this later
    redefined-outer-name, # to return later
    ungrouped-imports,  # return later
    anomalous-backslash-in-string  # return later